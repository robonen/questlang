квест МодульныйПример;
цель "Пример использования модулей в QuestLang";

импорт Локации из "./locations.ql";

граф {
	узлы {
		старт: {
			тип: начальный;
			описание: "Вы стоите на развилке";
			переходы: [выбор];
		}

		выбор: {
			тип: действие;
			описание: "Куда пойти?";
			варианты: [
				("В лес", @Локации.лес),
				("На гору", @Локации.гора)
			];
		}
	}

	начало: старт;
}

конец;
